msgstr "Typ"

#: src/tools/dcpomatic.cc:1125 src/tools/dcpomatic_batch.cc:231
msgid "Unfinished jobs"
msgstr "Nedokončené úlohy"

#: src/tools/dcpomatic_playlist.cc:293
msgid "Up"
msgstr "Nahahoru"

